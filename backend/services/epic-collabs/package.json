{
  "name": "epic-collabs",
  "description": "Epic Collabs API",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "gen-gql": "ts-node ./src/utils/generate-schema.ts",
    "dev": "serverless offline start -s stevenp",
    "build": "tsc",
    "deploy": "./bin/deploy",
    "lint": "tslint -c tslint.json 'src/**/*.ts' && yarn prettier:check",
    "lint:fix": "yarn run prettier:write && tslint -c tslint.json 'src/**/*.ts' --fix",
    "prettier:check": "prettier -l 'src/**/*.ts' --config .prettierrc",
    "prettier:write": "prettier -l --write 'src/**/*.ts' --config .prettierrc",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --silent",
    "test:watch": "jest --coverage --verbose --watch"
  },
  "dependencies": {
    "@sp-tools/kloud-config": "^0.0.5",
    "@sp-tools/kloud-logger": "^0.0.3",
    "@sp-tools/kloud-parameter": "^0.0.6",
    "@sp-tools/kloud-stack": "^0.0.6",
    "apollo-server-lambda": "^2.16.1",
    "dataloader": "^2.0.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.0.18",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.0",
    "pino": "^6.5.1",
    "serverless-dynamodb-parameters": "^1.0.1",
    "serverless-logless-plugin": "^0.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.159",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.27",
    "@types/pino": "^6.3.0",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "serverless": "^1.78.1",
    "serverless-offline": "^6.5.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-prune-plugin": "^1.4.3",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  }
}
