{
  "compilerOptions": {
    "outDir": ".dist",
    "baseUrl": "./src",
    "sourceMap": false,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "target": "es2019",
    "module": "commonjs",
    "lib": ["dom", "esnext"]
  },
  "exclude": ["node_modules", ".dist", "webpack"]
}
